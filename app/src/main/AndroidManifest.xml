<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.energysh.activityapidemo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ActivityApiDemo">
        <activity
            android:name=".kotlin.ui.prevContract.PrevCaptureVideoActivity"
            android:exported="true" />
        <activity
            android:name=".kotlin.ui.prevContract.PrevTakePictureActivity"
            android:exported="true" />
        <activity
            android:name=".kotlin.ui.prevContract.PrevStartActivityForResultActivity"
            android:exported="true" />
        <activity
            android:name=".kotlin.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".kotlin.ui.simpleimage.SimpleImageActivity" />
    </application>

</manifest>